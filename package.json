{
  "name": "@yoldi/i18n-sync",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "i18n-sync": "dist/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watchAll",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Vladislav Romanov",
  "module": "dist/i18n-sync.esm.js",
  "size-limit": [
    {
      "path": "dist/i18n-sync.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/i18n-sync.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/google-spreadsheet": "^3.0.2",
    "husky": "^4.3.8",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "commander": "^7.0.0",
    "google-auth-library": "^6.1.6",
    "google-spreadsheet": "^3.1.15"
  }
}
